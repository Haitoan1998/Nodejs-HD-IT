<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home page</title>
    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <div class="topnav">
      <a class="active" href="/">Home</a>
      <a href="/Create">Create Users</a>
    </div>
    <div class="list-users">
      <h2>Hoverable Table</h2>
      <table>
        <tr>
          <th>ID</th>
          <th>Email</th>
          <th>Name</th>
          <th>City</th>
          <th>Action</th>
        </tr>

        <% listUsers.forEach(function(item) { %>
        <tr>
          <td><%= item.id %></td>
          <td><%= item.email %></td>
          <td><%= item.name %></td>
          <td><%= item.city %></td>
          <td>
            <button><a href="edit-user/<%= item.id %>" %>>Edit</a></button>
            <button>Delete</button>
          </td>
        </tr>
        <% }) %>
      </table>
    </div>
  </body>
</html>
